{
  "name": "horns-ts",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && cp package.json dist/",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@oclif/core": "^4.7.2",
    "pkg": "^5.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.23",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "oclif": {
    "commands": "./commands"
  },
  "pkg": {
    "assets": [
      "dist/commands/**/*",
      "dist/utils/**/*",
      "dist/package.json"
    ],
    "targets": [
      "node20-linux-x64"
    ],
    "outputPath": "dist"
  }
}
